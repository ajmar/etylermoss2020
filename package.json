{
  "name": "etylermoss.com",
  "private": true,
  "description": "My website, built with Gatsby & TypeScript.",
  "version": "0.1.0",
  "author": "Eden Tyler-Moss <eden@etylermoss.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "^3.5.1",
